[Unit]
Description=Prometheus Graphite Exporter
After=network.target

[Service]
Type=simple
User={{ prometheus_exporters_common_user }}
Group={{ prometheus_exporters_common_group }}
ExecStart={{ prometheus_exporters_common_root_dir }}/graphite_exporter_current/graphite_exporter

SyslogIdentifier=prometheus_graphite_exporter
Restart=always

[Install]
WantedBy=multi-user.target
